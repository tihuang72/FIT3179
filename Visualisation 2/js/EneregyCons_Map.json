{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Energy Consumption per Capita",
    "width": 900,
    "height": 400,
    "projection": {"type": "equirectangular"},
    "data": {
      "url": "https://raw.githubusercontent.com/tihuang72/FIT3179/refs/heads/main/Visualisation%202/js/LGA_2016_AUST.json",
      "format": {"type": "topojson", "feature": "ne_110m_admin_0_countries"}
    },
    "transform": [
      {
        "lookup": "properties.LGA_NAME16",
        "from": {
          "data": {
            "url": "https://raw.githubusercontent.com/tihuang72/FIT3179/refs/heads/main/Visualisation%202/data/LGA%2B2016%2Bzones.csv"
          },
          "key": "LGA_NAME_2016",
          "fields": ["Residential Capacity (MW)"]
        }
      }
    ],
    "mark": {"type": "geoshape"},
    "encoding": {
      "color": {
        "field": "Residential Capacity (MW)",
        "type": "quantitative"
      },
      "tooltip": [
        {"field": "properties.NAME", "type": "nominal", "title": "Country"},
        {"field": "Residential Annual Energy (GWH)", "type": "quantitative"}
      ]
    }
  }